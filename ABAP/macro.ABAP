DEFINE  INSERT_TABLE.
  CHECK STRLEN(&1) >= 5.
  APPEND &1 TO &2.
  READ TABLE &2 ASSIGNING <FIRST> INDEX 1.
  CHECK SY-SUBRC = 0.
  <FIRST> = 'NEW ENTRY INSERTED ' && <FIRST>.

  WRITE
END-OF-DEFINITION.

DATA : LT TYPE  STRING_TABLE.
FIELD-SYMBOLS : <FIRST> TYPE STRING.

BREAK-POINT.
INSERT_TABLE 'JERRY1' LT.
INSERT_TABLE 'JERRY2' LT.
INSERT_TABLE 'SS' LT.